{
  "realm": "collab-draw",
  "enabled": true,
  "displayName": "Collab Draw (Local Dev)",
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "sslRequired": "none",
  "clients": [
    {
      "clientId": "collab-draw",
      "name": "Collab Draw",
      "protocol": "openid-connect",
      "enabled": true,
      "publicClient": false,
      "secret": "collab-draw-dev-secret",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:3080/api/auth/callback",
        "http://localhost:3080/*"
      ],
      "webOrigins": [
        "http://localhost:3080"
      ]
    },
    {
      "clientId": "collab-draw-api",
      "name": "Collab Draw API",
      "protocol": "openid-connect",
      "enabled": true,
      "publicClient": false,
      "secret": "collab-draw-api-dev-secret",
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false
    }
  ],
  "roles": {
    "client": {
      "collab-draw-api": [
        {
          "name": "uma_protection",
          "composite": false,
          "clientRole": true
        }
      ]
    }
  },
  "scopeMappings": [],
  "clientScopeMappings": {
    "realm-management": [
      {
        "client": "collab-draw-api",
        "roles": ["view-users", "query-users"]
      }
    ]
  }
}
